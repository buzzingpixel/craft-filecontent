{
    "name": "buzzingpixel/filecontent",
    "description": "Add flat file content capability to your CraftCMS site",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "filecontent"
    ],
    "authors": [
        {
            "name": "TJ Draper",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "craftcms/cms": "^3.0.0-beta.29",
        "roave/security-advisories": "dev-master",
        "symfony/filesystem": "^2.7 || ^3.0",
        "mnapoli/front-yaml": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\filecontent\\": "src/"
        }
    },
    "extra": {
        "name": "File Content",
        "handle": "filecontent",
        "developer": "TJ Draper",
        "developerUrl": "https://buzzingpixel.com",
        "schemaVersion": "0.0.1",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "buzzingpixel\\filecontent\\Filecontent"
    }
}
